{
  "version": 2,
  "name": "stocklink",
  "builds": [
    {
      "src": "apps/heatmap/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    },
    {
      "src": "apps/details/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/heatmap/(.*)",
      "dest": "/apps/heatmap/$1"
    },
    {
      "src": "/details/(.*)",
      "dest": "/apps/details/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/heatmap/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "apps/heatmap/api/**/*.js": {
      "runtime": "nodejs18.x"
    },
    "apps/details/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}